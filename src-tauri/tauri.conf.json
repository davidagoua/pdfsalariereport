{
    "$schema": "https://schema.tauri.app/v2.2.0/config.json",
    "productName": "performers-paie",
    "version": "0.1.0",
    "identifier": "com.performers.paie",
    "build": {
        "beforeDevCommand": "",
        "beforeBuildCommand": "",
        "devUrl": "http://localhost:8000",
        "frontendDist": "../app/static"
    },
    "app": {
        "windows": [
            {
                "title": "Performers Paie",
                "width": 1200,
                "height": 800,
                "resizable": true,
                "fullscreen": false
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ],
        "externalBin": [
            "binaries/server"
        ]
    }
}